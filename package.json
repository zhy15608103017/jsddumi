{
  "private": true,
  "name": "jusda-tools",
  "version": "0.1.0",
  "gitHooks": {
    "pre-commit": ""
  },
  "husky": {
    "hooks": {}
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prepare": "dumi setup",
    "bootstrap": "lerna bootstrap",
    "dev": "father dev",
    "start": "dumi dev",
    "build": "FS_LOGGER=none dumi build",
    "ci": "npm run lint && npm run compile && npm run test",
    "entry": "node ./scripts/entry.js",
    "compile": "father-build",
    "pub": "npm run compile && lerna publish --force-publish=*",
    "lint": "prettier packages/**/*.{ts,tsx} --check && stylelint packages/**/*.less",
    "format": "prettier packages/**/*.{ts,tsx} --write && stylelint packages/**/*.less --fix",
    "dependon": "nodemon --exec node pack.js",
    "workspace": "lerna run --stream --sort build"
  },
  "sideEffects": false,
  "dependencies": {
    "@amap/amap-jsapi-loader": "1.0.1",
    "@babel/runtime": "7.17.8",
    "@turf/turf": "5.1.6",
    "@types/react": "17.0.41",
    "animate.css": "4.1.1",
    "axios": "0.27.1",
    "crypto-js": "4.1.1",
    "dayjs": "1.11.1",
    "dumi-theme-default": "1.1.24",
    "ejs": "3.1.6",
    "express": "4.17.1",
    "fs-extra": "10.0.0",
    "js-cookie": "2.2.1",
    "json-bigint": "1.0.0",
    "lerna": "4.0.0",
    "nodemon": "2.0.12",
    "pre-commit": "1.2.2",
    "ramda": "0.28.0",
    "rc-picker": "2.6.7",
    "rc-util": "5.19.3",
    "react-draggable": "4.4.5",
    "react-infinite-scroll-component": "6.1.0",
    "react-infinite-scroller": "1.2.4",
    "react-intl-universal": "2.5.3",
    "react-svg": "14.0.1",
    "react-transition-group": "4.4.5",
    "rollup": "2.38.5",
    "slash": "5.0.0",
    "slash2": "2.0.0",
    "spark-md5": "3.0.2",
    "styled-components": "5.3.5",
    "umi-request": "1.3.9",
    "urlencode": "1.1.0"
  },
  "devDependencies": {
    "@jusda-tools/jusda-address": "1.3.0",
    "@jusda-tools/address": "1.3.0",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@umijs/lint": "4.0.0",
    "dumi": "2.0.16",
    "eslint": "8.23.0",
    "father": "4.1.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "3.0.0",
    "prettier-plugin-packagejson": "2.2.18",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "lerna": "4.0.0",
    "less-loader": "6.2.0",
    "rimraf": "3.0.2"
  }
}
